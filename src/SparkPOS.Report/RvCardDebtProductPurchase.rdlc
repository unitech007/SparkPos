  <Code>
Shared TotalSaldo As Double = 0
Shared Saldo As Double = 0
Shared CurrentSupplierId As String = ""

Public Function GetSaldo(ByVal SupplierId As String, ByVal Type As String, ByVal Value As Double) As Double
	If (CurrentSupplierId &lt;&gt; SupplierId) Then
		CurrentSupplierId = SupplierId				
		Saldo = 0
		TotalSaldo = 0
	End If		
	
	If (Type = "1") Then
		Saldo = Saldo + value
	Else
		Saldo = Saldo - value
	End If
	
	TotalSaldo = TotalSaldo + Saldo
	
	Return Saldo
End Function

Public Function GetTotalSaldo() As Double			
	Return TotalSaldo
